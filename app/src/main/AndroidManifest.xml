<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bhumihar.mafiv">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/iiitv"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About_us"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".User_Profile" />
        <activity android:name=".User_Activity" />
        <activity android:name=".Time_Table" />
        <activity android:name=".Week_Activity" />
        <activity android:name=".Time_Activity" />
        <activity android:name=".Bus_Time_Activity" />
        <activity android:name=".Mess_Time_Activity" />
        <activity android:name=".Notification_Activity" />
        <activity android:name=".Set_Remainder" />
        <activity android:name=".View_Remainder" />
        <activity
            android:name=".About_Remainder"
            android:theme="@style/Theme.AppCompat.Dialog"></activity>
        <activity android:name=".Alarm_Activity">
        <intent-filter>
            <action android:name="android.intent.action.ALARM_ACTIVITY" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        </activity>
        <activity android:name=".Library_activity"/>
        <activity android:name=".CreatePoll"/>
        <activity android:name=".EditMyPoll"/>
        <activity android:name=".FillForm"/>
        <activity android:name=".ViewMyPoll"/>
        <activity android:name=".PollFeature"/>
        <activity android:name=".Poll_Results"/>
        <activity android:name=".ViewActivePolls"/>


        <receiver android:name=".Alarm_Receiver"></receiver>
        <service android:name=".RingtonePlayService"
            android:enabled="true"
            />

    </application>


</manifest>